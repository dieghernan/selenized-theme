{
  "name": "Selenized Light Theme",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#FBF3DB",
        "foreground": "#53676D"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#909995"
      }
    },
    {
      "name": "Generic Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Quoted String",
      "scope": "string.quoted.docstring",
      "settings": {
        "foreground": "#909995"
      }
    },
    {
      "name": "Special elements in doc comments",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#909995",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#009C8F"
      }
    },
    {
      "name": "String interpolation and escaping",
      "scope": [
        "constant.character.escape",
        "constant.character.format",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "string.constant.character.escape",
        "string.interpolated.backtick.shell",
        "string.interpolated.dollar.shell"
      ],
      "settings": {
        "foreground": "#D2212D"
      }
    },
    {
      "name": "Code embedded in string interpolation",
      "scope": "meta.embedded",
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.language",
        "constant.numeric",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#009C8F"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Generic Keywords",
      "scope": [
        "keyword.control",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Function and class defs",
      "scope": [
        "storage.type",
        "storage.type.class",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "keyword.control.directive.include",
        "keyword.control.import",
        "keyword.other.import"
      ],
      "settings": {
        "foreground": "#C25D1E"
      }
    },
    {
      "name": "Imported package and module names",
      "scope": [
        "storage.modifier",
        "storage.modifier.import.java"
      ],
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Types and type modifiers",
      "scope": [
        "storage",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#AD8900"
      }
    },
    {
      "name": "Keyword argument name in function call",
      "scope": "variable.parameter.function-call",
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Reserved variables like this, super, self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#D2212D"
      }
    },
    {
      "name": "Built-ins, things from library and framework",
      "scope": "support",
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Exceptions",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#AD8900"
      }
    },
    {
      "name": "Function decorator and similar elements",
      "scope": [
        "entity",
        "entity.name.function.decorator",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Name of the defined class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Name of the defined function",
      "scope": [
        "entity.name.function",
        "entity.name.function.declaration"
      ],
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Function calls",
      "scope": [
        "meta",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Function calls and defs use the same scope in JS",
      "scope": "entity.name.function.js",
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Ordinary variables",
      "scope": "variable",
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Shell builtins",
      "scope": "support.function.builtin.shell",
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Special shell syntax",
      "scope": [
        "punctuation.definition.logical-expression",
        "variable.other.special.shell"
      ],
      "settings": {
        "foreground": "#D2212D"
      }
    },
    {
      "name": "Shell variable interpolation",
      "scope": [
        "variable.other.bracket.shell",
        "variable.other.normal.shell",
        "variable.other.positional.shell"
      ],
      "settings": {
        "foreground": "#C25D1E"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#CC1729",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.link",
        "meta.link"
      ],
      "settings": {
        "foreground": "#009C8F"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "entity.name.section.markdown",
        "markup.heading",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#A78300"
      }
    },
    {
      "name": "Other supports",
      "scope": "support.function",
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Markup Code Blocks",
      "scope": [
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#0072D4"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS selectors and ids",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "HTML attributes",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "YAML rules",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "YAML directive",
      "scope": "storage.type.tag.verbatim.yaml",
      "settings": {
        "foreground": "#0072D4"
      }
    }
  ],
  "colors": {
