{
  "name": "Selenized Dark Theme",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#103C48",
        "foreground": "#ADBCBC"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#FF665C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "atrule",
      "scope": [
        "entity.name.tag.yaml",
        "keyword.control.at-rule",
        "meta.at-rule",
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#75B938"
      }
    },
    {
      "name": "attr-name",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#75B938"
      }
    },
    {
      "name": "attr-value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "boolean",
      "scope": [
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "class-name",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "code",
      "scope": [
        "text.html.markdown markup.inline.raw",
        "text.html.markdown markup.raw"
      ],
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "comment",
      "scope": "comment",
      "settings": {
        "foreground": "#72898F"
      }
    },
    {
      "name": "content",
      "scope": "meta.embedded.line",
      "settings": {
        "foreground": "#ED8649"
      }
    },
    {
      "name": "constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "doctype",
      "scope": [
        "meta.tag.metadata.doctype",
        "meta.tag.sgml.doctype.html",
        "meta.tag.sgml.html"
      ],
      "settings": {
        "foreground": "#72898F"
      }
    },
    {
      "name": "function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "important",
      "scope": [
        "constant.numeric.yaml-version",
        "entity.other.document",
        "keyword.other.directive",
        "keyword.other.important",
        "meta.directives",
        "punctuation.definition.alias",
        "punctuation.definition.anchor",
        "punctuation.section.embedded",
        "variable.other.alias",
        "variable.other.anchor"
      ],
      "settings": {
        "foreground": "#75B938",
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "markup.inline.raw",
        "markup.raw",
        "storage"
      ],
      "settings": {
        "foreground": "#75B938"
      }
    },
    {
      "name": "keyword-null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "namespace,",
      "scope": [
        "entity.name.type.namespace",
        "storage.modifier.import"
      ],
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "operator",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.class",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.relational",
        "keyword.other",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#75B938"
      }
    },
    {
      "name": "property",
      "scope": [
        "meta.property-list",
        "support.type.property-name",
        "support.type.property-name.css",
        "support.type.vendored.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#DBB32D"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#ADBCBC"
      }
    },
    {
      "name": "regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "selector",
      "scope": [
        "meta.selector",
        "source.css entity.name.tag",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#75B938"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#41C7B9"
      }
    },
    {
      "name": "tag",
      "scope": [
        "meta.document.yaml",
        "meta.tag",
        "storage.type.tag"
      ],
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "title",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#EBC13D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "variable",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#ADBCBC"
      }
    },
    {
      "name": "url",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "CSS text",
      "scope": [
        "meta.property-list.css",
        "source.css keyword.other"
      ],
      "settings": {
        "foreground": "#4695F7"
      }
    },
    {
      "name": "SCSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#ED8649"
      }
    }
  ],
  "colors": {
